{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/react-router": "^5.1.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^6.5.2",
    "formik": "^1.5.8",
    "history": "^4.10.1",
    "husky": "^3.0.7",
    "lint-staged": "^9.4.0",
    "node-sass": "^7.0.0",
    "prettier": "^1.18.2",
    "react": "^16.10.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.10.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.1",
    "react-scripts": "3.1.2",
    "redux": "^4.0.4",
    "redux-act": "^1.7.7",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.6.3",
    "yup": "^0.27.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "additional-rule": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "additional-typescript-only-rule": "warn"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
